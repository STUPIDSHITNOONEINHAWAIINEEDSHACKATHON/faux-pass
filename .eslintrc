{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
    "async-await",
    "import",
    "transform-async-to-generator"
  ],
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "async-await/space-after-async": 2,
    "async-await/space-after-await": 2
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.base.js"
      }
    }
  }
}
